{{#ifCond 0 count}}

<nav aria-label="Page navigation">
  <ul class="pagination">
 
    {{#ifCond 0 prevPages}}
        <li><a href="{{url}}?page=1"><<</a></li>

       
    {{/ifCond}}

    {{#ifCond 1 pageStart}}
    <li><a href="{{url}}?page={{prev}}">Previous</a></li>
    {{/ifCond}}

    {{#forLoop url pageStart pageEnd}}
    <li><a href="?page={{this}}">{{this}}</a></li>
    {{/forLoop}}

    {{#ifLessEqual next numPages}}
    <li><a href="{{url}}?page={{next}}">Next</a></li>
    {{/ifLessEqual}}

    {{#ifCond 0 numPages}}
        <li><a href="{{url}}?page={{numPages}}">>></a></li>
   
    {{/ifCond}}

 </ul>
</nav>


{{/ifCond}}
