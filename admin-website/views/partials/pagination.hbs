
<h5>Pagination come here</h5>
<div>

{{#ifCond 0 prevPages}}
    <a href="/items/list/?page={{prevPages}}"><<</a>
{{/ifCond}}

{{#forLoop pageStart numPages}}
    <a href="/items/list/?page={{this}}">{{this}}</a>
{{/forLoop}}

{{#ifCond nextPages numPages}}
    <a href="/items/list/?page={{numPages}}">>></a>
{{/ifCond}}

</div>